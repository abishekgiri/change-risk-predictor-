{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "json": {
      "()": "releasegate.server.JsonLogFormatter"
    }
  },
  "handlers": {
    "default": {
      "class": "logging.StreamHandler",
      "formatter": "json",
      "stream": "ext://sys.stdout"
    },
    "access": {
      "class": "logging.StreamHandler",
      "formatter": "json",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "uvicorn": {
      "handlers": [
        "default"
      ],
      "level": "INFO",
      "propagate": false
    },
    "uvicorn.error": {
      "handlers": [
        "default"
      ],
      "level": "INFO",
      "propagate": false
    },
    "uvicorn.access": {
      "handlers": [
        "access"
      ],
      "level": "INFO",
      "propagate": false
    }
  }
}
